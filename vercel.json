{
  "version": 2,
  "functions": {
    "api/scheduled-task.py": {
      "runtime": "vercel-python",
      "schedule": "*/5 * * * *"
    }
  },
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/main.py"
    }
  ]
}
